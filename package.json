{
  "name": "playalong",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cannedfood/Playalong.git"
  },
  "pwaManifest": {
    "name": "Playalong",
    "theme": "#2196f3",
    "generateIconOptions": {
      "baseIcon": "./icon.png",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "scripts": {
    "build-frontend": "parcel build web/index.html --dist-dir wwwroot/",
    "serve-frontend": "parcel web/index.html --dist-dir wwwroot/",
    "release-ubuntu.20.04-arm64": "npm run clean && npm run build-frontend && dotnet publish -c Release -r ubuntu.20.04-arm64 -o publish/ubuntu.20.04-arm64",
    "start": "./scripts/start.sh",
    "clean": "rm -R wwwroot .parcel-cache/ obj/ bin/"
  },
  "author": "Benno Straub",
  "license": "GPLv3",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "get-youtube-id": "^1.0.1",
    "parcel-config-pwa-manifest": "0.0.8",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "youtube-player": "^5.5.2"
  },
  "devDependencies": {
    "@types/youtube-player": "^5.5.3",
    "@vue/compiler-sfc": "^3.0.5",
    "less": "^4.1.1",
    "parcel": "^2.0.0-nightly.558",
    "pug": "^3.0.0"
  }
}
